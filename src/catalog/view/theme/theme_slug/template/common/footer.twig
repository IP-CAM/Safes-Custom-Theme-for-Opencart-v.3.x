  <div class="page__footer">
    <div class="wrapper">
      <footer class="footer">
        <div class="footer__row">
          <a
            class="footer-telephone"
            href="tel:{{ telephone|replace({'-':"",'(':"",')':"",' ':""}) }}"
          >
            {{ telephone }}
          </a>
        </div>
        <div class="footer__row">
          <ul class="menu menu_type_footer-navigation">
          {% for information in informations %}
            <li class="menu__item">
              <a
                class="menu__link link link_theme_standard"
                href="{{ information.href }}"
              >
                {{ information.title }}
              </a>
            </li>
          {% endfor %}
          </ul>
        </div>
      </footer>
    </div>
  </div>
  <div class="page__notifications">
    <div class="wrapper notifications">
    </div>
  </div>
